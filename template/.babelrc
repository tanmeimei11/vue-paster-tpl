{
  "presets": [
    ["env",{
      "modules": false,
       "targets": {
         "browsers": ["last 2 versions", "safari >= 7"]
       }
     }], "stage-0"
  ],
  "plugins": [
    "transform-decorators-legacy",
    ["transform-runtime", {
      "helpers": false,
      "polyfill": false,
      "regenerator": true
    }]
  ]
}
