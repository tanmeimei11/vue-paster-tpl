style.
  #global-loading{position:fixed;width:100%;height:100%;background-color:#fff;z-index:999}
  .global-loading{margin:0 auto;padding-top:3.333333rem;overflow:hidden;width:5.333333rem;height:5.333333rem;background-image:url(data:image/jpg;base64,);background-size:contain;background-position:center bottom;background-repeat:no-repeat}.global-loading .global-loading-logo{width:1.6rem;height:1.6rem;margin:2.466667rem auto 0;background-color:#ff4545;border-radius:50%;background-image:url(data:image/png;base64,);background-size:.8rem .6rem;background-repeat:no-repeat;background-position:center;-webkit-animation:moveball 1s ease-out infinite;animation:moveball 1s ease-out infinite}.global-loading .global-loading-shadow{width:1.2rem;height:.173333rem;margin:.226667rem auto 0;-webkit-transform-origin:50% 50%;transform-origin:50% 50%;background-color:#f1f5f8;border-radius:50%;-webkit-animation:movebot 1s ease-out infinite;animation:movebot 1s ease-out infinite}@-webkit-keyframes moveball{0%{-webkit-transform:translate(0,-50%);transform:translate(0,-50%)}50%{-webkit-transform:translate(0,0);transform:translate(0,0)}100%{-webkit-transform:translate(0,-50%);transform:translate(0,-50%)}} @keyframes moveball{0%{-webkit-transform:translate(0,-50%);transform:translate(0,-50%)}50%{-webkit-transform:translate(0,0);transform:translate(0,0)}100%{-webkit-transform:translate(0,-50%);transform:translate(0,-50%)}}@-webkit-keyframes movebot{0%{-webkit-transform:scale(.5,1);transform:scale(.5,1)}50%{-webkit-transform:scale(1.3,1);transform:scale(1.3,1)}100%{-webkit-transform:scale(.5,1);transform:scale(.5,1)}} @keyframes movebot{0%{-webkit-transform:scale(.5,1);transform:scale(.5,1)}50%{-webkit-transform:scale(1.3,1);transform:scale(1.3,1)}100%{-webkit-transform:scale(.5,1);transform:scale(.5,1)}}
