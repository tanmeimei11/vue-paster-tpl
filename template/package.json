{
  "name": "{{ name }}",
  "version": "1.0.0",
  "description": "{{ description }}",
  "author": "{{ author }}",
  "private": true,
  "config": {
    "docker":{
      "volume":"${PWD}:/usr/src/app",
      "port":"8018:8018",
      "image":"jiuyan/vuetpl:dva"
    },
    "compress":["src/assets/img/**/**.*", "src/assets/qiniu/**/**.*"],
    "qiniu": ["src/assets/qiniu/**/**.*"]
  },
  "scripts": {
    "start": "npm run dev",
    "dev": "node build/exec.js dev-server",
    "build": "node build/exec.js build"
  },
  "dependencies": {
    "common-utils": "git+ssh://git@githost.in66.cc/unpkg/common-utils",
    {{#if_eq analytics "yes"}}
    "analytics": "git+ssh://git@githost.in66.cc/unpkg/analytics",
    {{/if_eq}}
    {{#if_eq extends.swiper true}}
    "vue-awesome-swiper": "^2.5.4",
    {{/if_eq}}
    "vue": "^2.5.2",
    "dva-vue": "^0.0.7",
    "core-js": "^2.5.1",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "chalk": "^2.1.0",
    "css-loader": "^0.28.7",
    "eslint": "^4.5.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^3.2.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.15.4",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.30.1",
    "http-proxy-middleware": "^0.17.4",
    "imports-loader": "^0.7.1",
    "node-sass": "^4.5.3",
    "postcss": "^6.0.11",
    "postcss-loader": "^2.0.6",
    "sass-loader": "^6.0.6",
    "shelljs": "^0.7.8",
    "url-loader": "^0.5.9",
    "vue-loader": "^13.0.4",
    "vue-template-compiler": "^2.4.2",
    "webpack": "^3.5.5",
    "webpack-config": "^7.4.0",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.18.2"
  }
}
